import 'dart:io';
void main(){
  double result;
  //Input values
  print("Enter Total Bill");
  int? totalbill=int.parse(stdin.readLineSync()!);
  print("Enter total persons");
  int? persons=int.parse(stdin.readLineSync()!);
  //Compute result
  result=totalbill/persons;
  print("Individual amount to be paid is $result");
}
