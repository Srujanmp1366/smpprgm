import 'dart:io';
void main(){
  //Variables for calculation
  int costperkilo,totalcost;
  //Input values
  print("Enter destination");
  String? destination=stdin.readLineSync();
  print("Enter weight");
  int? weight=int.parse(stdin.readLineSync()!);
  //Compute total cost
  switch(destination){
    case 'XYZ':
      costperkilo=5;
      totalcost=costperkilo*weight;
      print("Shipping cost is $totalcost");
    case 'ABC':
      costperkilo=7;
      totalcost=costperkilo*weight;
      print("Shipping cost is $totalcost");
    case 'PQR':
      costperkilo=10;
      totalcost=costperkilo*weight;
      print("Shipping cost is $totalcost");
    default:
      print("Invalid entry");
      
  }
  
}
